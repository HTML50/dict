{
  "manifest_version": 2,
  "name": "dict",
  "description": "an easiest way to look up English word",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "version": "1.3",
  "permissions": [
    "storage", "tabs"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "global": true
    },
    "open-dict-global": {
      "suggested_key": {
        "default": "Alt+Shift+1"
      },
      "description": "在浏览器外打开字典的全局快捷键",
      "global": true
    }

  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "css": ["css/inject.css"],
    "js": ["js/content-script.js"],
    "matches": ["<all_urls>"],
    "all_frames": true
  }, {
    "css": ["css/dict.cn.css"],
    "js": ["js/dict.cn.js"],
    "matches": ["http://dict.cn/*", "http://www.dict.cn/*"],
    "run_at": "document_end",
    "all_frames": true
  }, {
    "css": ["css/youdao.css"],
    "js": ["js/youdao.js"],
    "matches": ["https://youdao.com/w/eng/*", "http://youdao.com/w/eng/*", "http://www.youdao.com/w/eng/*", "https://www.youdao.com/w/eng/*", "https://www.youdao.com/w/*", "https://youdao.com/w/*", "http://www.youdao.com/w/*", "http://youdao.com/w/*"],
    "run_at": "document_end",
    "all_frames": true
  }, {
    "css": ["css/eudic.net.css"],
    "js": ["js/eudic.net.js"],
    "matches": ["https://dict.eudic.net/dicts/en/*", "http://dict.eudic.net/dicts/en/*"],
    "run_at": "document_end",
    "all_frames": true
  }],
  "update_url": "https://raw.githubusercontent.com/HTML50/dict/master/update",
  "web_accessible_resources": ["404.html"]
}
